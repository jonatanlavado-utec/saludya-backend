# Docker Compose Environment Variables
# These are used by docker-compose.yml

# PostgreSQL Configuration
POSTGRES_USER=saludya
POSTGRES_PASSWORD=saludya123
POSTGRES_DB=saludya_db

# Service Database URLs (automatically set by docker-compose)
# All services use the same database for simplicity
DATABASE_URL=postgresql://saludya:saludya123@postgres:5432/saludya_db

# Service Ports
AUTH_SERVICE_PORT=8001
USER_SERVICE_PORT=8002
CATALOG_SERVICE_PORT=8003
APPOINTMENT_SERVICE_PORT=8004
AI_SERVICE_PORT=8005
PAYMENT_SERVICE_PORT=8006

# Database Port
DATABASE_PORT=5432
